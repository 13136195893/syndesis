{{^Productized}}
{{^WithDockerImages}}
- apiVersion: v1
  kind: ImageStream
  metadata:
    name: {{ Images.Syndesis.Rest }}
    labels:
      app: syndesis
      type: infrastructure
      component: syndesis-server
  spec:
    tags:
    - from:
        kind: DockerImage
        name: ${SYNDESIS_REGISTRY}/{{ Images.SyndesisImagesPrefix }}/{{ Images.Syndesis.Rest }}:{{ Tags.Syndesis }}
      importPolicy:
        scheduled: true
      name: "{{ Tags.Syndesis }}"
