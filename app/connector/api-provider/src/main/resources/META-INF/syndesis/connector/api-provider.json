{
  "id": "api-provider",
  "name": "API Provider",
  "description": "Expose Restful APIs",
  "icon": "data:image/svg+xml;base64,",
  "dependencies": [
    {
      "type": "MAVEN",
      "id": "@project.groupId@:@project.artifactId@:@project.version@"
    }
  ],
  "actions": [
    {
      "name": "Provided API",
      "description": "Start a Syndesis integration from a provided API",
      "id": "io.syndesis:api-provider-start",
      "pattern": "From",
      "actionType": "connector",
      "tags": ["expose"],
      "metadata": {
        "serverBasePath": "/api"
      },
      "descriptor": {
        "componentScheme": "direct",
        "inputDataShape": {
          "kind": "none"
        },
        "outputDataShape": {
          "kind": "any"
        },
        "propertyDefinitionSteps": [
          {
            "description": "API Provider Configuration",
            "name": "configuration",
            "properties": {
              "name": {
                "componentProperty": false,
                "deprecated": false,
                "description": "The operation ID as defined in the API spec",
                "displayName": "Operation ID",
                "javaType": "String",
                "kind": "parameter",
                "required": true,
                "secret": false,
                "type": "string"
              }
            }
          }
        ]
      }
    },
    {
      "name": "Provided API Return Path",
      "description": "End action of Syndesis integrations that start from a provided API",
      "id": "io.syndesis:api-provider-end",
      "pattern": "To",
      "actionType": "connector",
      "descriptor": {
        "componentScheme": "log",
        "inputDataShape": {
          "kind": "any"
        },
        "outputDataShape": {
          "kind": "any"
        },
        "propertyDefinitionSteps": [
          {
            "description": "API Provider Return Path Configuration",
            "name": "configuration",
            "properties": {
              "loggerName": {
                "componentProperty": false,
                "deprecated": false,
                "description": "The return code to set in the HTTP response",
                "displayName": "Return Code",
                "javaType": "String",
                "kind": "parameter",
                "required": true,
                "secret": false,
                "type": "string"
              }
            }
          }
        ]
      }
    }
  ]
}
