      app: syndesis
      component: syndesis-verifier
  spec:
    tags:
    - from:
        kind: DockerImage
        name: {{Registry}}/syndesis/syndesis-verifier:{{Tag}}
      importPolicy:
        scheduled: true
      name: {{Tag}}
{{/Dev}}
