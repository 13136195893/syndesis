{{^Dev}}
- apiVersion: v1
  kind: ImageStream
  metadata:
    name: syndesis-rest
    labels:
      app: syndesis
      component: syndesis-rest
  spec:
    tags:
    - from:
        kind: DockerImage
        name: {{Registry}}/syndesis/syndesis-rest:{{Tag}}
      importPolicy:
        scheduled: true
      name: {{Tag}}
- apiVersion: v1
  kind: ImageStream
  metadata:
    name: syndesis-keycloak
    labels:
      app: syndesis
      component: syndesis-keycloak
  spec:
    tags:
    - from:
        kind: DockerImage
        name: {{Registry}}/jimmidyson/keycloak-openshift:2.5.4.Final
      importPolicy:
        scheduled: true
      name: 2.5.4.Final
- apiVersion: v1
  kind: ImageStream
  metadata:
