        name: {{Registry}}/syndesis/token-rp:v0.6.2
      importPolicy:
        scheduled: true
      name: v0.6.2
- apiVersion: v1
  kind: ImageStream
  metadata:
    name: syndesis-atlasmap
    labels:
      app: syndesis
      component: syndesis-atlasmap
  spec:
    tags:
    - from:
        kind: DockerImage
        name: {{Registry}}/atlasmap/atlasmap:{{Tag}}
