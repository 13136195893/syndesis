{
  "name": "ipaas-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "generate": "node scripts/generate-ts.js",
    "start": "yarn ng serve",
    "lint": "yarn ng lint",
    "lint:fix": "yarn ng lint -- --fix",
    "test": "yarn ng test",
    "test:cc": "yarn ng test -- --watch=false --cc",
    "test:xvfb": "docker run --rm -v `pwd`:/ipaas-ui:z -w /ipaas-ui -u `id -u` docker.io/rhipaas/karma-xvfb ./karma-xvfb.sh",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "docker:build": "yarn ng build -- --aot --prod && docker build -t rhipaas/ipaas-ui:latest ."
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "@ng2-dynamic-forms/core": "1.3.13",
    "@ng2-dynamic-forms/ui-bootstrap": "1.3.13",
    "angular-oauth2-oidc-hybrid": "1.0.22",
    "angular2-toaster": "2.0.0",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "immutable": "3.8.1",
    "jquery": "3.1.1",
    "lodash": "4.17.4",
    "ng2-bootstrap": "1.3.2",
    "ng2-restangular": "0.1.27",
    "patternfly": "4.0.0-alpha.2",
    "pluralize": "3.1.0",
    "reselect": "2.5.4",
    "rxjs": "5.0.3",
    "tether": "1.4.0",
    "ts-helpers": "1.1.2",
    "typescript-logging": "0.2.0-beta5",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.31",
    "@angular/compiler-cli": "2.4.3",
    "@types/jasmine": "2.5.40",
