    "angular2-toaster": "2.0.0",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "ipaas.data.mapper": "https://github.com/redhat-ipaas/datamapper-ui.git#08c46fd1b905017d193f1db085b2d08e4dc4247c",
